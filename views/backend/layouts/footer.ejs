<script type="text/javascript">
    $(document).ready(function () {
        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').toggleClass('active');
        });
        setTimeout(function () {
            hideNotifications();
        }, 2500);
        var hideNotifications = function () {
            $(".alert").slideUp(600, function () {
                $(this).remove();
            })
        }
    });
</script>
