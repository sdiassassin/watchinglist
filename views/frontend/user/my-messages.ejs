<!DOCTYPE html>
<html lang="en">

<%- include ../layouts/header.ejs %>

<body>

<%- include ../layouts/menubar.ejs %>


<section class="page-content">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h3>My messages</h3>
            </div>
        </div>
        <hr/>
        <div class="row">
            <div class="col-lg-12">
                <div class="table-messages">
                    <div class="table-messages-thead">
                        <div class="table-messages-thead-tr">
                            <div class="table-messages-thead-th">&nbsp;</div>
                            <div class="table-messages-thead-th">&nbsp;</div>
                            <div class="table-messages-thead-th">Name</div>
                            <div class="table-messages-thead-th col-title">Title</div>
                            <div class="table-messages-thead-th">Date</div>
                        </div>
                    </div>
                    <div class="table-messages-tbody">
                        <div class="table-messages-tbody-tr">
                            <div class="table-messages-tbody-td">
                                <div class="table-messages-tbody-td-inner">
                                    <div class="table-checkbox"></div>
                                </div>
                            </div>
                            <div class="table-messages-tbody-td">
                                <div class="table-messages-tbody-td-inner">
                                    <div class="listing-thumb"><img src="http://i.pravatar.cc/100" data-bg="true" /></div>
                                </div>
                            </div>
                            <a href="/my-messages-conversation" class="table-messages-tbody-td">
                                <div class="table-messages-tbody-td-inner">
                                    <div>John Doe</div>
                                </div>
                            </a>
                            <a href="/my-messages-conversation" class="table-messages-tbody-td col-title">
                                <div class="table-messages-tbody-td-inner">
                                    <div class="listing-title">2018 Rolex Celini</div>
                                </div>
                            </a>
                            <a href="/my-messages-conversation" class="table-messages-tbody-td">
                                <div class="table-messages-tbody-td-inner">
                                    <div class="listing-price">21 Dec, 2018 - 3:45 pm</div>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
                <div class="listing-actions">
                    <div><a href="#" class="btn btn-primary">Delete</a></div>
                </div>
            </div>
        </div>
    </div>
</section>

<hr/>


<%- include ../layouts/footer.ejs %>

</body>
</html>
